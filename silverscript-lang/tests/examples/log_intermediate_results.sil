pragma silverscript ^0.1.0;

contract LogIntermediateResults(pubkey owner) {
    entrypoint function test_log_intermediate_result() {
        byte[32] singleHash = sha256(owner);
        byte[32] doubleHash = sha256(singleHash);
        require(doubleHash.length == 32, "doubleHash should be 32 bytes");
    }
}
