pragma silverscript ^0.1.0;

contract Test(int x, string y) {
    entrypoint function hello(int a, string b) {
        if (a == x - 2) {
            require(false);
        } else if (b == "Hello " + y)
            require(y == "World");
        else {
            require(true == !!!false);
        }
    }
}
